{
    "version": "0.2.0",
    "configurations": [{
        "type": "chrome",
        "runtimeExecutable": "/usr/bin/chromium",
        "runtimeArgs": [
            "--remote-debugging-port=9222"
        ],
        "request": "launch",
        "name": "Chromium localhost",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceRoot}",
        "sourceMaps": false,
        "smartStep": false
    }]
}
